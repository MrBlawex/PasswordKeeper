<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="120.0" prefWidth="320.0" stylesheets="@css/style.css"
            xmlns="http://javafx.com/javafx/8" fx:controller="passwordkeeper.controller.LoginFormWindowController">
    <HBox alignment="CENTER" layoutX="10.0" layoutY="10.0" prefHeight="31.0" prefWidth="321.0" spacing="6.0"
          AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
        <Label alignment="CENTER" layoutX="14.0" layoutY="32.0" prefHeight="21.0" prefWidth="72.0" text="Пароль"
               AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="10.0"/>
        <PasswordField fx:id="passwordField" layoutX="94.0" layoutY="27.0" prefHeight="31.0" prefWidth="230.0"
                       stylesheets="@css/style.css" AnchorPane.leftAnchor="94.0" AnchorPane.topAnchor="10.0"/>
    </HBox>
    <Button fx:id="btn_back" cancelButton="true" layoutX="8.0" layoutY="129.0" mnemonicParsing="false"
            onMouseClicked="#closeWindow" prefWidth="75.0" text="Отмена" AnchorPane.bottomAnchor="10.0"
            AnchorPane.leftAnchor="10.0"/>
    <Button fx:id="btn_enter" defaultButton="true" layoutX="258.0" layoutY="129.0" mnemonicParsing="false"
            prefWidth="75.0" text="Войти" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="10.0"/>
    <Label fx:id="lb_message" alignment="CENTER" layoutX="158.0" layoutY="50.0" prefHeight="21.0" prefWidth="330.0"
           textAlignment="CENTER" textFill="RED" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0"/>
</AnchorPane>
